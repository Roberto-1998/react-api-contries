{
    "version":2,
    "name":"WorldCountries",
    "builds":[
        {
            "use":"@vercel/static-build",
            "src":"package.json"
        }
    ],
    "routes":[
        {
            "src":"(.*).js",
            "dest":"/$1.js"
        },
        {
            "src":"/.*",
            "dest":"/ index.html"
        }
    ]
}